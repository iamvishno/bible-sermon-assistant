    "@babel/core": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.57.1",
